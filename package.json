{
  "name": "mellon-website",
  "version": "0.1.0",
  "keywords": [
    "IIIF",
    "manifest",
    "redux",
    "react"
  ],
  "description": "A customizable React and Redux based front-end to present collections of IIIF manifests.",
  "dependencies": {
    "@hesburgh-wse/eslint-config-hesburgh": "^1.0.0",
    "@sentry/browser": "^4.3.0",
    "eslint-config-hesburgh": "https://github.com/ndlib/eslint-config-hesburgh.git",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.7.0",
    "react-markdown": "^4.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-router-scroll-top": "^0.1.1",
    "react-select": "^2.0.0",
    "react-waypoint": "^8.1.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sentry": "^0.1.2",
    "typy": "3.0.1",
    "url-loader": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "coveralls": "^3.0.2",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "fetch-mock": "^7.2.5",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-fetch": "^2.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "raw-loader": "^0.5.1",
    "redux-mock-store": "^1.5.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "webpack": "4.19.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "node node_modules/eslint/bin/eslint.js ./src",
    "test": "node scripts/test.js",
    "test-inline": "node scripts/test-inline.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/enzyme.config.js",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
